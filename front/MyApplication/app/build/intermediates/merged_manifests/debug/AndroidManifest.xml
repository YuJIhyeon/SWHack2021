<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.hack.sw_hack"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="16"
        android:targetSdkVersion="30" />

    <application
        android:allowBackup="true"
        android:appComponentFactory="androidx.core.app.CoreComponentFactory"
        android:debuggable="true"
        android:icon="@drawable/icon_ver1"
        android:label="@string/app_name"
        android:roundIcon="@drawable/icon_ver1"
        android:supportsRtl="true"
        android:testOnly="true"
        android:theme="@style/Theme.SW_Hack" >
        <activity android:name="com.hack.sw_hack.SignUpActivity" />
        <activity android:name="com.hack.sw_hack.MainActivity" />
        <activity android:name="com.hack.sw_hack.StartActivity" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>